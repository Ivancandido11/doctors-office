<h1><%= @patient.name%></h1>
<% @patient.doctors.uniq.each do |doctor|%>
  <p>
    <%= link_to doctor.name, doctor%>: <br>
      <% doctor.appointments.where(patient_id: @patient.id).each do |appt|%>
        <%= appt.date.strftime("%B %e, %Y")%>:
        <%= appt.notes %>
        <br>
        <br>
    <% end %>
  </p>
<% end %>

